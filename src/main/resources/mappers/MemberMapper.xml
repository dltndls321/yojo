<mapper namespace="memberMapperNS">
  <insert id="insertMember" parameterType="memberModel">
    	Insert Into member(memnum,id,passwd,name,jumin,address,phone,email,regdate)
    	values(
    		memberSeq.NEXTVAL,
    		#{id},
    		#{passwd},
    		#{name},
    		#{jumin},
    		#{address},
    		#{phone},
    		#{email},
    		sysdate
    	)
    </insert>
