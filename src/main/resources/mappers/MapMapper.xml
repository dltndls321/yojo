    	select * from food
   </select>
     
   <select id="getFoodList2"  parameterType="hashmap"  resultType="mapModel" >
   	 <![CDATA[
    select * from food where foodx>#{startX} and foodx<#{endX} and foody>#{startY} and foody<#{endY}
